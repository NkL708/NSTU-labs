                styleClass="innerPane">
        <VBox fx:id="votesPane" prefHeight="292.0" prefWidth="221.0"/>
    </ScrollPane>
    <ImageView fx:id="backToMenuButton" fitHeight="49.0" fitWidth="49.0" layoutX="18.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../pictures/arrow.png" />
    </ImageView>
</AnchorPane>
